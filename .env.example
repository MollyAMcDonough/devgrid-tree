
# Public variables (exposed in browser)
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Not currently used in production, need to update page.tsx and [id]>page.tsx from SOCKET_SERVER_URL to NEXT_PUBLIC_SOCKET_SERVER_URL for dev
# NEXT_PUBLIC_SOCKET_SERVER_URL=http://localhost:4000

# Private variables (not exposed to browser)
DATABASE_URL=postgresql://postgres:password@localhost:5432/mydb

# Socket server URL for API routes (server-to-server)
SOCKET_SERVER_URL=http://localhost:4000

# CORS origin for socket server (set to frontend URL in prod)
CORS_ORIGIN=http://localhost:3000

# Example secret keys (replace in production)
SECRET_KEY=your-secret-key
SENTRY_DSN=https://xxxxxxx@sentry.io/123456

# (Optional) Test database URL for running tests
# DATABASE_URL="postgresql://postgres:password@localhost:5432/mydb_test"
